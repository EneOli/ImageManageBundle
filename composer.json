{
	"name": "creavio/image-manage-bundle",
	"type": "symfony-bundle",
	"description": "Symfony Bundle to resize images inside of twig",
	"keywords": ["image", "resize", "twig", "symfony2", "bundle", "creavio"],
	"homepage": "http://github.com/creavio/ImageManageBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Malte Frerichs",
			"email": "frerichs@creavio.com"
		}
	],
	"require": {
		"php":                      ">=7.1",
		"symfony/framework-bundle": "~2.3|~3.0|~4.0|~5.0",
		"twig/twig":                "~1.12|~2|~3"
	},
	"require-dev": {
		"symfony/expression-language": "~2.4|~3.0|~4.0"
	},
	"autoload": {
		"psr-4": {
			"Creavio\\ImageManageBundle\\": ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
